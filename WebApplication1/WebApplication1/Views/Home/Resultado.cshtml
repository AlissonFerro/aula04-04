@using WebApplication1
@using WebApplication1.Models
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model List<Resposta>

<!DOCTYPE html>
<html>
<head>
    <title>Inicio</title>
</head>
<body>
    @foreach (Resposta r in Model)
    {
        <h1>Nomes</h1>
        <p>@r.Name</p>
    }

    <p>O número total de nomes cadastrados é: @Model.Count</p>

    <a asp-action="Index">Voltar para o início</a>
</body>
</html>
